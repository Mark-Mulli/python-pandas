   "source": [
    "import pandas as pd\n",
    "import os\n",
    "#import time\n",
    "import time\n",
    "#import console\n",
    "from py_console import console\n",
    "#define location\n",
    "#You can use your defined pathname as well\n",
    "location = '/Users/markmulli/Desktop/Scripting/raw_files'\n",
    "#declare a dataframe\n",
    "output_ntta = pd.DataFrame(columns=[\"LP\",\"State\",\"Vehicle\",\"Dept\",\"Exit date & time\",\"Exit Plaza\",\"Amount(No multiplier)\",\"Amount(With multiplier)\"])\n",
    "def process_ntta (filename):\n",
    "    filename.split(\"/\")[-1].split(\".\")[0]\n",
    "    \n",
    "    #An Empty list that stores columns of a dataframe",
    "    output_list = []\n",
    "    \n",
    "    curr_df = pd.read_excel(filename)\n",
    "    \n",
    "    for iter,rows in curr_df.iterrows():\n",
    "    #create a dictionary",
    "        row_dict = {\n",
    "            \"TollTag ID\":\"\",\n",
    "            \"Plate\":\"\",\n",
    "            \"State\": \"\",\n",
    "            \"Vehicle#\": \"\",\n",
    "            \"Dept\": \"\",\n",
    "            \"Transaction Date/Time\": \"\",\n",
    "            \"Location\": \"\",\n",
    "            \"Amount(No multiplier)\": \"\",\n",
    "            \"Amount(With multiplier)\": \"\",\n",
    "        }\n",
    "        \n",
    "        row_dict[\"State\"] = str(rows[\"Plate\"]).split(\" - \")[1].strip()\n",
    "        row_dict[\"Plate\"] =  str(rows[\"Plate\"]).split(\" - \")[0].strip()\n",
    "        \n",
    "        if pd.isnull(rows[\"TollTag ID\"]):\n",
    "            row_dict[\"TollTag ID\"] = \"\"\n",
    "        else:\n",
    "            row_dict[\"TollTag ID\"] = str(rows[\"TollTag ID\"])\n",
    "    \n",
    "        \n",
    "        row_dict[\"Transaction Date/Time\"] = (\n",
    "            str(rows[\"Transaction Exit Date/Time\"])\n",
    "            .replace(r'=TEXT(\"', \"\")\n",
    "            .replace(r'\",\"mm/dd/yyyy HH:mm:SS\")', \"\")\n",
    "            .replace(r'=Text(\"', \"\")\n",
    "        )\n",
    "        row_dict[\"Location\"] = str(rows[\"Location\"]).split(\" - \")[-1]\n",
    "\n",
    "        row_dict[\"Amount(No multiplier)\"] = (\n",
    "            str(rows[\"Transaction Amount\"])\n",
    "            .replace(\"$\", \"\")\n",
    "            .replace(\"-\", \"\")\n",
    "            .replace(\"(\", \"\")\n",
    "            .replace(\")\", \"\")\n",
    "        )\n",
    "        row_dict[\"Amount(With multiplier)\"] = (  \n",
    "            str(rows[\"Transaction Amount\"])  \n",
    "            .replace(\"$\", \"\")\n",
    "            .replace(\"-\", \"\")\n",
    "            .replace(\"(\", \"\")\n",
    "            .replace(\")\", \"\")\n",
    "        )\n",
    "        output_list.append(row_dict)\n",
    "    \n",
    "    df = pd.DataFrame(output_list)\n",
    "        \n",
    "    df = df.astype({\"Amount(With multiplier)\":\"float64\",\"Amount(No multiplier)\":\"float64\",\"Transaction Date/Time\":\"datetime64[ns]\"})\n",
    "        \n",
    "    return df\n",
    "    \n",
    "#start time\n",
    "start = time.time()\n",
    "final_df = output_ntta\n",
    "\n",
    "unprocessed = []\n",
    "\n",
    "for i in os.listdir(location):\n",
    "    if i.lower().endswith(\".xlsx\"):\n",
    "        if i.startswith(\"NTTA\"):\n",
    "            print(i)\n",
    "            final_df = pd.concat([final_df,process_ntta(location +\"/\"+ i)],ignore_index=True)\n",
    "            print(\"!! processed :\" + i)\n",
    "        else:\n",
    "            unprocessed.append(i)\n",
    "    else:\n",
    "        unprocessed.append(i)\n",
    "datatoexcel = pd.ExcelWriter(f'output_files/modified.xlsx',engine=\"xlsxwriter\")\n",
    "\n",
    "final_df[[\"Amount(With multiplier)\"]] = final_df[[\"Amount(With multiplier)\"]]*1.41\n",
    "\n",
    "final_df.to_excel(datatoexcel,index=False)\n",
    "\n",
    "datatoexcel.close()\n",
    "\n",
    "#stop time\n",
    "stop = time.time()\n",
    "#output time\n",
    "console.success(f'Done processing in {stop-start}')\n"
    
    "            \n",
    "            \n",
    "            \n",
    "    "
   ]
